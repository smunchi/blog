        "lodash": "^4.17.19",
        "popper.js": "^1.16.1",
        "postcss": "^8.1.14",
        "resolve-url-loader": "^2.3.2",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "vue-template-compiler": "^2.6.13",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.0",
        "webpack-livereload-plugin": "^1.2.0"
    },
    "dependencies": {
        "cross-env": "^7.0.3"
    }
